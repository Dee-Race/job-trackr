<h1>Your Current Job Applications</h1>

<%= link_to 'See all job titles', user_job_titles_path(@user) %>



<% @user.applications.each do |application| %>


    <p><%= application.company %></p>
    <p><%= application.job_title.title %></p>
    <p><%= application.job_location %></p>
    <p><%= application.job_salary %></p>
    <p><%= application.job_url %></p>
    <p><%= application.description %></p>
    <p><%= application.date_applied %></p>
    <% end %>

  

   <p><%= link_to 'Add new job application', new_application_path %></h3></p>
